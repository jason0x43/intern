{
  "name": "intern",
  "version": "4.0.0-pre",
  "description": "Intern. A next-generation code testing stack for JavaScript.",
  "homepage": "http://theintern.io/",
  "license": "BSD-3-Clause",
  "bugs": "https://github.com/theintern/intern/issues",
  "files": [
    "bin",
    "chai.js",
    "client.html",
    "client.js",
    "lib",
    "main.js",
    "order.js",
    "runner.js",
    "support/fixdeps.js",
    "tasks",
    "tests/example.intern.js",
    "typings"
  ],
  "keywords": [
    "javascript",
    "test",
    "unit",
    "testing",
    "ci",
    "continuous integration",
    "bdd",
    "tdd",
    "xunit",
    "istanbul",
    "chai",
    "dojo",
    "toolkit",
    "selenium",
    "sauce labs",
    "code coverage"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/theintern/intern.git"
  },
  "files": [
    "Node.*",
    "WebDriver.*",
    "index.*",
    "bin/*.js",
    "browser",
    "lib",
    "loaders",
    "tasks",
    "tests",
    "types"
  ],
  "bin": {
    "intern": "./bin/intern.js"
  },
  "scripts": {
    "build": "intern-dev-clean && stylus src/lib/reporters/html/html.styl && intern-dev-lint && intern-dev-build",
    "postbuild": "intern-dev-browserify",
    "browserify": "intern-dev-browserify",
    "clean": "intern-dev-clean",
    "lint": "intern-dev-lint",
    "release": "intern-dev-release",
    "test": "node _tests/src/bin/intern.js"
  },
  "dependencies": {
    "@types/benchmark": "~1.0.30",
    "@types/chai": "~3.4.34",
    "@types/charm": "~1.0.0",
    "@types/source-map": "~0.1.28",
    "@types/ws": "~0.0.38",
    "benchmark": "2.1.1",
    "chai": "3.5.0",
    "charm": "0.2.0",
    "diff": "1.1.0",
    "digdug": "https://github.com/jason0x43/digdug/releases/download/2.0.0-pre/digdug-2.0.0-pre.tgz",
    "dojo-core": "2.0.0-alpha.19",
    "dojo-interfaces": "2.0.0-alpha.10",
    "glob": "7.0.3",
    "istanbul": "0.4.1",
    "leadfoot": "https://github.com/jason0x43/leadfoot/releases/download/2.0.0-pre/leadfoot-2.0.0-pre.tgz",
    "lodash-amd": "4.16.4",
    "mime-types": "2.1.13",
    "platform": "1.3.1",
    "source-map": "0.1.33",
    "ws": "2.1.0"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/chokidar": "^1.4.29",
    "@types/diff": "^0.0.31",
    "@types/glob": "^5.0.30",
    "@types/grunt": "^0.4.20",
    "@types/node": "^6.0.0",
    "@types/shelljs": "^0.3.32",
    "@types/systemjs": "^0.20.2",
    "browserify": "^14.1.0",
    "dojo-loader": "2.0.0-beta.7",
    "dojo-has": "2.0.0-alpha.6",
    "dojo-shim": "2.0.0-beta.7",
    "intern-dev": "https://github.com/theintern/intern-dev/releases/download/0.1.10/intern-dev-0.1.10.tgz",
    "systemjs": "0.20.11",
    "tslint": "^4.4.0",
    "typescript": "^2.2.0"
  },
  "internDev": {
    "resources": {
      "_build": [
        "src/**/*.{css,d.ts,html,js,png}",
        "src/**/!(tsconfig).json"
      ],
      "_build/src": [
        "types/**",
        "bin/**"
      ],
      "_tests": [
        "tests/**/*.xml",
        "src/**/*.{css,d.ts,html,js,png}"
      ]
    },
    "browserify": {
      "_build/src/browser/intern.js": [
        "_build/src/browser/intern.js"
      ],
      "_build/src/browser/remote.js": [
        "_build/src/browser/remote.js"
      ],
      "_tests/src/browser/intern.js": [
        "_tests/src/browser/intern.js"
      ],
      "_tests/src/browser/remote.js": [
        "_tests/src/browser/remote.js"
      ]
    }
  }
}
